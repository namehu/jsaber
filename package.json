{
  "name": "jsaber",
  "version": "0.1.7",
  "description": "",
  "main": "dist/jsaber.umd.js",
  "module": "dist/jsaber.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "gulp buildTypes",
    "build:js": "gulp",
    "pub": "git push --follow-tags origin master && npm publish",
    "release": "standard-version",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namehu/jsaber.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/namehu/jsaber/issues"
  },
  "homepage": "https://github.com/namehu/jsaber#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-loader": "^8.0.6",
    "cz-conventional-changelog": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.2",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "standard-version": "^7.1.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
